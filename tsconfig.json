{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*", "src/*"]
    },
    "types": ["react-router-dom"]
  },
  "include": ["src/**/*.js", "src/**/*.tsx", "src/index.tsx"],
  "exclude": ["node_modules"]
}
